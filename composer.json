{
    "name": "andikaryanto11/laravelorm",
    "description": "ORM Database mapping for Laravel",
    "keywords": ["laravelorm", "laravel", "ORM","Database"],
    "homepage": "https://packagist.org/packages/andikaryanto11/laravelorm",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "symfony/yaml": "^5.4",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "require-dev": {
		"phpunit/phpunit": "^9.5.9",
		"codeception/specify": "^1.4",
		"codeception/verify": "^2.1",
		"phpspec/prophecy-phpunit": "^2.0",
        "mockery/mockery": "^1.5"
	},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LaravelOrm\\": "src/"
        }
    }

}
